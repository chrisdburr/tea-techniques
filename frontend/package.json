{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "dev:static": "NEXT_PUBLIC_DATA_SOURCE=static next dev",
    "dev:api": "NEXT_PUBLIC_DATA_SOURCE=api next dev",
    "dev:mock": "NEXT_PUBLIC_DATA_SOURCE=mock next dev",
    "build": "NEXT_DISABLE_LINT=true next build",
    "build:static": "pnpm validate:build static && pnpm sync-data && pnpm generate-api && NEXT_PUBLIC_DATA_SOURCE=static NEXT_DISABLE_LINT=true next build && pnpm validate:post-build",
    "build:dynamic": "pnpm validate:build api && pnpm sync-data && NEXT_PUBLIC_DATA_SOURCE=api next build && pnpm validate:post-build",
    "build:mock": "pnpm validate:build mock && pnpm sync-data && pnpm generate-api && NEXT_PUBLIC_DATA_SOURCE=mock next build && pnpm validate:post-build",
    "build:github-pages": "pnpm build:static && touch out/.nojekyll",
    "analyze": "cross-env ANALYZE=true NODE_ENV=production next build",
    "analyze-deps": "pnpm list --depth=1",
    "analyze-size": "du -sh node_modules",
    "start": "next start",
    "start:static": "node scripts/serveStatic.mjs",
    "lint": "next lint",
    "generate-api": "node scripts/generateStaticApi.js",
    "sync-data": "node scripts/syncTechniqueData.js",
    "sync-data:check": "node scripts/syncTechniqueData.js --check",
    "validate-api": "node scripts/validateStaticData.js",
    "validate:build": "node scripts/validateBuild.js",
    "validate:post-build": "node scripts/postBuildValidation.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "quality-check": "pnpm type-check && pnpm lint && pnpm test:coverage",
    "pre-commit": "pnpm quality-check && pnpm build",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.6.5",
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-query": "^5.66.10",
    "@types/prismjs": "^1.26.5",
    "axios": "^1.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "lucide-react": "^0.476.0",
    "next": "15.2.0",
    "next-themes": "^0.4.6",
    "prettier": "^3.6.2",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.55.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.2",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.2.3",
    "@playwright/test": "^1.40.1",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.5.1",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "canvas": "^3.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.2.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest-axe": "^9.0.0",
    "jsdom": "^25.0.0",
    "jsdom-global": "^3.0.2",
    "msw": "^2.10.3",
    "postcss": "^8.5.3",
    "shadcn": "^2.4.0-canary.9",
    "tailwindcss": "^4.0.9",
    "typescript": "^5",
    "vite": "^5.4.14",
    "vitest": "^2.1.8"
  },
  "overrides": {
    "inflight": "npm:lru-cache@^7.14.1"
  }
}
